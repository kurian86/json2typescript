  "keywords": [
    "convert",
    "json",
    "to",
    "typescript",
    "javascript",
    "map",
    "mapper",
    "mapping",
    "json2typescript",
    "jsonconvert",
    "class",
    "object",
    "instance"
  ],
  "author": "dhlab@unibas.ch",
  "license": "MIT",
  "homepage": "http://dhlab.unibas.ch",
  "scripts": {
    "test": "karma start karma.conf.js",
